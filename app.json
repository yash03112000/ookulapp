{
	"expo": {
		"name": "Ookul",
		"slug": "ookulapp",
		"scheme": "ookulapp",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/logo.png",
		"splash": {
			"image": "./assets/logo.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"androidStatusBar": {
			"backgroundColor": "#5c02ab",
			"translucent": false
		},
		"jsEngine": "hermes",
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.ookulapp.ookulapp"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/logo.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.ookulapp"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
